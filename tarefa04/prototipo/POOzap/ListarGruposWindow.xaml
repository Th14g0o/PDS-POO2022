<Window x:Class="POOzap.ListarGruposWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POOzap"
        mc:Ignorable="d"
        Title="UADIZAPI" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="8*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <ListBox Margin="10"/>
        <ListBox Grid.Column="1" Margin="10"/>
        <Button Content="LISTAR" Grid.Column="0" Grid.Row="1" Margin="10"/>
        <Button Content="SAIR DO GRUPO" Grid.Column="1" Grid.Row="1" Margin="10"/>
    </Grid>
</Window>
